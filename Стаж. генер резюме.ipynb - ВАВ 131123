        "resume_texts"
      ],
      "metadata": {
        "id": "0IHDfpFS66R_"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def structure_text(resume_texts, splitter):\n",
        "    docs = {}\n",
        "    for topic_text in resume_texts.split(splitter):     # \"\\r\\n# \"\n",
        "        topic, *text = topic_text.split(\"\\n\")\n",
        "        text = \"\\n\".join(text)\n",
        "        topic = topic.strip()\n",
        "        if docs.get(topic, False):\n",
        "            docs[topic] += \"\\n\" + text\n",
        "            continue\n",
        "\n",
        "        docs[topic] = \"\\n\" + text\n",
        "    return docs"
      ],
      "metadata": {
        "id": "UNSDCi-W6roe"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
